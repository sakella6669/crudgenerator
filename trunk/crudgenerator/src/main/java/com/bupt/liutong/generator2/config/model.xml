<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.w3school.com.cn" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.w3school.com.cn model.xsd">
	<package name="bbs">
		<table name="bbs_board" clazz="Board" chs="版块">
			<column name="id" field="id" chs="版块id" dbType="int" javaType="Integer" length="11"
				inputItem="false" isPrimary="true" autoIncrement="true" />
			<column name="parentId" field="parentId" chs="父版块id" dbType="int" javaType="Integer"
				length="11" defaultValue="-1" />
			<column name="name" field="name" chs="版块名称" dbType="varchar" javaType="String"
				length="60" itemType="10" mustInput="true" listDisp="true" />
			<column name="des" field="des" chs="描述" dbType="text" javaType="String"
				itemType="40" />
			<column name="enabled" field="enabled" chs="是否显示" dbType="tinyint" javaType="Integer"
				length="1" itemType="50" listDisp="true" codeMapping="ifDisp" />
			<column name="seq" field="seq" chs="顺序号" dbType="tinyint" javaType="Integer" length="4"
				defaultValue="10" itemType="20" listDisp="true" />
			<column name="needPassword" field="needPassword" chs="是否需要密码" dbType="tinyint" javaType="Integer"
				length="1" itemType="50" listDisp="true" codeMapping="yesOrNo" />
			<column name="password" field="password" chs="密码" dbType="varchar" javaType="String"
				length="30" itemType="10" />
			<column name="auth" field="auth" chs="是否需要密码" dbType="tinyint" javaType="Integer"
				length="1" itemType="50" listDisp="true" codeMapping="yesOrNo" />
			<column name="level" field="level" chs="版块层级" dbType="tinyint" javaType="Integer" length="2" />
			<column name="mainPostNum" field="mainPostNum" chs="发帖数" dbType="int" javaType="Integer" length="11" />
			<column name="postNum" field="postNum" chs="总帖数" dbType="int" javaType="Integer" length="11" />
		</table>
		<table name="bbs_post" clazz="Post" chs="帖子">
			<import clazz="java.util.Date" />
			<import clazz="com.bupt.liutong.util.DateUtils" />
			<column name="id" field="id" chs="帖子id" dbType="bigint" javaType="Long" length="20"
				inputItem="false" isPrimary="true" autoIncrement="true" />
			<column name="parentId" field="parentId" chs="主帖id" dbType="bigint" javaType="Long"
				length="20" inputItem="false" />
			<column name="boardId" field="boardId" chs="板块id" dbType="int" javaType="Integer"
				length="11" inputItem="false" />
			<column name="title" field="title" chs="标题" dbType="varchar" javaType="String"
				length="150" itemType="10" mustInput="true" queryItem="true" listDisp="true" />
			<column name="replyNum" field="replyNum" chs="回复数" dbType="int" javaType="Integer"
				length="11" inputItem="false" />
			<column name="clickNum" field="clickNum" chs="点击数" dbType="int" javaType="Integer"
				length="11" inputItem="false" />
			<column name="userName" field="userName" chs="发帖人id" dbType="varchar" javaType="String"
				length="30" itemType="10" listDisp="true" />
			<column name="nickName" field="nickName" chs="发帖人" dbType="varchar" javaType="String"
				length="30" itemType="10" listDisp="true" />
			<column name="content" field="content" chs="帖子内容" dbType="text" javaType="String"
				itemType="40" listDisp="true" />
			<column name="createTime" field="createTime" chs="发帖时间" dbType="datetime" javaType="Date"
				itemType="30" listDisp="true" />
			<column name="ip" field="ip" chs="ip" dbType="char" javaType="String"
				length="15" itemType="10" />
			<column name="lastReplyTime" field="lastReplyTime" chs="最后回帖时间" dbType="datetime" javaType="Date"
				itemType="30" listDisp="true" />
			<column name="lastUserName" field="lastUserName" chs="最后发帖人id" dbType="varchar" javaType="String"
				length="30" itemType="10" />
			<column name="lastNickName" field="lastNickName" chs="最后发帖人" dbType="varchar" javaType="String"
				length="30" itemType="10" />
			<column name="elite" field="elite" chs="是否精华" dbType="tinyint" javaType="Integer"
				length="1" itemType="50" listDisp="true" codeMapping="yesOrNo" />
			<column name="top" field="top" chs="是否置顶" dbType="tinyint" javaType="Integer"
				length="1" itemType="50" listDisp="true" codeMapping="yesOrNo" />
			<column name="lock" field="lock" chs="是否锁定" dbType="tinyint" javaType="Integer"
				length="1" itemType="50" listDisp="true" codeMapping="yesOrNo" />
			<column name="hidden" field="hidden" chs="是否隐藏" dbType="tinyint" javaType="Integer"
				length="1" itemType="50" listDisp="true" codeMapping="yesOrNo" />
		</table>
	</package>
</model>