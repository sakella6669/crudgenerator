		<template path="src/main/java/com/bupt/liutong/generator2/template/gen_action.java.xsl"
			targetPath="src/main/java/com/bupt/liutong/${package}/action/gen/Gen${table.clazz}Action.java"
			override="true" />
		<template path="src/main/java/com/bupt/liutong/generator2/template/list.jsp.xsl"
			targetPath="WebRoot/${package}/${table.clazz.lower}/${table.name}_list.jsp" />
		<template path="src/main/java/com/bupt/liutong/generator2/template/list_head.jsp.xsl"
			targetPath="WebRoot/${package}/${table.clazz.lower}/${table.name}_list_head.jsp" override="true" />
		<template path="src/main/java/com/bupt/liutong/generator2/template/list_query.jsp.xsl"
			targetPath="WebRoot/${package}/${table.clazz.lower}/${table.name}_list_query.jsp" override="true" />
		<template path="src/main/java/com/bupt/liutong/generator2/template/list_result.jsp.xsl"
