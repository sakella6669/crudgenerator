		<template path="src/main/java/com/bupt/liutong/generator2/template/insert.jsp.xsl"
			targetPath="WebRoot/${package}/${table.clazz.lower}/${table.name}_insert.jsp" />
		<template path="src/main/java/com/bupt/liutong/generator2/template/insert_head.jsp.xsl"
			targetPath="WebRoot/${package}/${table.clazz.lower}/${table.name}_insert_head.jsp" override="true" />
		<template path="src/main/java/com/bupt/liutong/generator2/template/insert_input.jsp.xsl"
			targetPath="WebRoot/${package}/${table.clazz.lower}/${table.name}_insert_input.jsp" override="true" />
		<template path="src/main/java/com/bupt/liutong/generator2/template/update.jsp.xsl"
			targetPath="WebRoot/${package}/${table.clazz.lower}/${table.name}_update.jsp" />
