			targetPath="WebRoot/WEB-INF/web.xml" transform="model" override="true" />
		<template path="src/main/java/com/bupt/liutong/generator2/template/.project.xsl"
			targetPath=".project" transform="config" />
		<template path="src/main/java/com/bupt/liutong/generator2/template/pom.xml.xsl"
			targetPath="pom.xml" transform="config" />
		<template path="src/main/java/com/bupt/liutong/generator2/template/ibatis.xml.xsl"
			targetPath="src/main/resources/ibatis.xml" transform="model" override="true" />
		<template path="src/main/java/com/bupt/liutong/generator2/template/struts.xml.xsl"
			targetPath="src/main/resources/struts.xml" transform="model" override="true" />
		<template path="src/main/java/com/bupt/liutong/generator2/template/struts-package.xml.xsl"
			targetPath="src/main/resources/struts-${package}.xml" transform="package" override="true" />
		<template path="src/main/java/com/bupt/liutong/generator2/template/spring.xml.xsl"
			targetPath="src/main/resources/spring.xml" transform="config" override="true" />
		<template path="src/main/java/com/bupt/liutong/generator2/template/spring-gen-package.xml.xsl"
			targetPath="src/main/resources/spring-gen-${package}.xml" transform="package" override="true" />
