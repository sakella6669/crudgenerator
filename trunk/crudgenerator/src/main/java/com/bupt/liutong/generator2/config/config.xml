		<template path="src/main/java/com/bupt/liutong/generator2/template/update_input.jsp.xsl"
			targetPath="WebRoot/${package}/${table.clazz.lower}/${table.name}_update_input.jsp" override="true" />
		<template path="src/main/java/com/bupt/liutong/generator2/template/ddl.sql.xsl"
			targetPath="sql/${table.name}.sql" override="true" />
			
